pages = {304--309},
publisher = {IEEE},
shorttitle = {Computer Communications and Networks, 2001. Procee},
title = {{A scalable solution to minimum cost forwarding in large sensor networks}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=956276},
