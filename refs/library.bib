keywords = {Buildings,Digital photography,Image analysis,Image retrieval,Image sensors,Information retrieval,Mobile ad hoc networks,Routing protocols,Wireless application protocol,Wireless networks,ad hoc networks,image processing,imaging devices,information dissemination protocols,meta data,metadata queries,mobile radio,picture sharing,routing protocol},
pages = {913--916},
publisher = {IEEE},
shorttitle = {Image Processing. 2002. Proceedings. 2002 Internat},
title = {{SPIN-IT: a data centric routing protocol for image retrieval in wireless networks}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=1039121},
volume = {1},
year = {2002}
}
@article{Buttussi2008,
