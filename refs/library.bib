keywords = {Bluetooth,Electronic learning,Explosives,Frequency,Internet,Internet telephony,Metropolitan area networks,Mobile computing,Voice over IP,WiFi,WiMAX,WiMAX.,Wireless LAN,Wireless personal area networks,broadband networks,broadband wireless technologies,e-Learning,multimedia application,multimedia systems,ubiquitous computing,wireless data transmission,wireless local area networks,wireless metropolitan area networks},
month = aug,
pages = {14--14},
publisher = {IEEE},
shorttitle = {Wireless Broadband and Ultra Wideband Communicatio},
title = {{Analogy of Promising Wireless Technologies on Different Frequencies: Bluetooth, WiFi, and WiMAX}},
